{
  "name": "@aws-sdk-workshop/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for the AWS JS SDK v3 workshop",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^1.0.0-gamma.2",
    "@aws-sdk/client-s3": "^1.0.0-gamma.2",
    "@aws-sdk/credential-provider-cognito-identity": "^1.0.0-gamma.2",
    "@aws-sdk/s3-request-presigner": "^1.0.0-gamma.1",
    "@aws-sdk/util-create-request": "^1.0.0-gamma.1",
    "@aws-sdk/util-format-url": "^1.0.0-gamma.1",
    "@reach/router": "1.3.3",
    "react": "16.13.1",
    "react-bootstrap": "1.0.1",
    "react-dom": "16.13.1"
  },
  "scripts": {
    "prepare:frontend": "cd .. && yarn prepare:frontend",
    "build": "react-scripts build",
    "build:frontend": "cd .. && yarn build:frontend",
    "start": "react-scripts start",
    "start:frontend": "cd .. && yarn start:frontend",
