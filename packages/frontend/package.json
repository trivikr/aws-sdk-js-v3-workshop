    "cdk": "cd .. && yarn cdk"
  },
  "keywords": [
    "aws",
    "JavaScript",
    "TypeScript",
    "SDK",
    "v3",
    "workshop",
    "Node.js"
  ],
  "author": "Kamat, Trivikram",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.12.24",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "http-server": "0.12.3",
    "react-scripts": "3.4.0",
    "typescript": "~3.9.3"
  }
